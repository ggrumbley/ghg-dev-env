{
  "name": "ghg-dev-env",
  "version": "1.0.0",
  "description": "ES6 development environment",
  "scripts": {
    "predev": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src",
    "open:src": "babel-node buildScripts/srcServer.js",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000 --subdomain gary",
    "lint": "esw webpack.config.* src buildScripts",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "dev": "npm-run-all --parallel security-check open:src lint:watch test:watch",
    "dev:open": "npm-run-all --parallel security-check open:src localtunnel"
  },
  "repository": "git@github.com:ggrumbley/ghg-dev-env.git",
  "author": "gary <ggrumbley@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "chalk": "^1.1.3",
    "cheerio": "^1.0.0-rc.1",
    "compression": "^1.6.2",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.0.0",
    "eslint-plugin-import": "^2.5.0",
    "eslint-watch": "^3.1.2",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "^0.5.0-rc6",
    "json-server": "^0.10.1",
    "localtunnel": "^1.8.3",
    "mocha": "^3.4.2",
    "nock": "^9.0.13",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "numeral": "^2.0.6",
    "open": "^0.0.5",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "surge": "^0.19.0",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "^0.0.5"
  }
}
